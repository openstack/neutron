msgid "ip command failed on device %(dev_name)s: %(reason)s"
msgstr "Не удалось выполнить команду ip на устройстве %(dev_name)s: %(reason)s"

#, python-format
msgid "ip command failed: %(reason)s"
msgstr "Команда ip не выполнена: %(reason)s"

