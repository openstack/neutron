msgid "BgpDrAgent %(id)s is invalid or has been disabled."
msgstr "BgpDrAgent %(id)s no es válido o se ha deshabiltado."

#, python-format
msgid "BgpDrAgent updated: %s"
msgstr "BgpDrAgent actualizado: %s"

msgid "Body contains invalid data"
